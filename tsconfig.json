{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "types": ["node"],
    "baseUrl": ".",
    "moduleResolution": "bundler",
    "module": "ESNext",
    "target": "ESNext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "#app": [".nuxt/types/app"],
      "#imports": [".nuxt/imports"],
      "~/*": ["./*"],
      "@/*": ["./*"],
      "~~/*": ["./*"],
      "@@/*": ["./*"]
    },
    "typeRoots": [
      "./node_modules/@types",
      "./types"
    ]
  },
  "files": [],
  "include": ["types/**/*.d.ts"]
}
